{
  "name": "passport-simple-webauthn2",
  "version": "2.0.0",
  "description": "Passport strategy for authenticating with Web Authentication (WebAuthn) using FIDO2 Passkeys.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint --fix src",
    "docs": "typedoc --out docs src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubentalstra/passport-simple-webauthn.git"
  },
  "keywords": [
    "typescript",
    "webauthn",
    "passkeys",
    "fido",
    "node",
    "passport"
  ],
  "author": "Ruben Talstra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rubentalstra/passport-simple-webauthn/issues"
  },
  "homepage": "https://github.com/rubentalstra/passport-simple-webauthn#readme",
  "peerDependencies": {
    "express": "^4.17.0",
    "express-session": "^1.17.0",
    "passport": "^0.7.0"
  },
  "dependencies": {
    "@simplewebauthn/server": "^13.1.1",
    "base64url": "^3.0.1",
    "passport-strategy": "^1.0.0",
    "redis": "^4.7.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/passport-strategy": "^0.2.38",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "engines": {
    "node": ">=21"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
